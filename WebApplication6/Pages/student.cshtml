@page
@model WebApplication6.Pages.studentModel
@{
}
<h1>Student list</h1>
<table class="table">
    <thead>
        <tr>
             <th>ID</th>
            <th>NAME</th>
            <th>COURSE</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model.listofstudents)
        {
        <tr>
            <td>@student.id</td>
            <td>@student.name</td> 
             <td>@student.course</td>
           
        </tr>
        }
    </tbody>
</table>









